<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="net.realqinwei.hzcrm.crm.been.Image" table="images">
		<id name="imageId" type="java.lang.Integer">
			<column name="image_id" />
			<generator class="identity" />
		</id>
		<property name="imageName" type="java.lang.String">
			<column name="image_name" length="1024" not-null="false" />
		</property>
		<property name="imageUrl" type="java.lang.String">
			<column name="image_url" length="1024" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
